{
    "name": "gco/kong-client-admin-api",
    "description": "Client PHP for kong admin api",
    "require": {
        "php": "^7.1",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench" : "~3.5.0|~3.6.0|~3.7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gco\\KongApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gco\\KongApiClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gco\\KongApiClient\\GcoKongApiClientServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "guillaume-cozic",
            "email": "guillaume.cozic@gmail.com"
        }
    ]
}
